{
  "name": "daily-spin",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --dotenv .env.local",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "format": "biome check --write --unsafe .",
    "lint": "tsc --build --noEmit && biome check .",
    "test:db:up": "docker compose up -d --wait && dotenv -e .env.test -- prisma migrate deploy",
    "test:db:down": "docker compose down -v",
    "test:integration:run": "vitest run --project integration",
    "test:integration": "bun run test:db:up && bun test:integration:run && bun run test:db:down",
    "test:unit": "vitest run --project unit",
    "test:component": "vitest run --project component"
  },
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "@nuxt/image": "2.0.0",
    "@nuxt/ui": "^4.1.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@spotify/web-api-ts-sdk": "^1.2.0",
    "@types/node": "^25.0.3",
    "better-auth": "^1.4.10",
    "consola": "^3.4.2",
    "date-fns": "^4.1.0",
    "luxon": "^3.7.2",
    "nuxt": "^4.2.2",
    "pg": "^8.16.3",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@electric-sql/pglite": "^0.3.14",
    "@faker-js/faker": "^10.2.0",
    "@iconify-json/lucide": "^1.2.84",
    "@nuxt/test-utils": "^3.23.0",
    "@testing-library/vue": "^8.1.0",
    "@types/lodash.merge": "^4.6.9",
    "@types/pg": "^8.16.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/ui": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "happy-dom": "^20.0.11",
    "lodash.merge": "^4.6.2",
    "prisma": "^7.2.0",
    "vitest": "^4.0.16"
  }
}
